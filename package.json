{
  "name": "mindbudget",
  "version": "2.0.0",
  "description": "Gestão financeira pessoal com análise emocional de gastos",
  "main": "node_modules/expo/AppEntry.js",
  "author": {
    "name": "Arthur Parreiras",
    "email": "arthurparreiras07@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arthurparreiras-07/app-gestao-financeira.git"
  },
  "license": "MIT",
  "keywords": [
    "react-native",
    "expo",
    "typescript",
    "finance",
    "budget",
    "emotional-analysis",
    "sqlite"
  ],
  "scripts": {
    "start": "expo start",
    "start:clear": "expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:apk": "eas build -p android --profile preview",
    "build:prod": "eas build -p android --profile production",
    "build:local": "expo prebuild --platform android && cd android && ./gradlew assembleRelease",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf node_modules && rm -rf .expo && npm install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "date-fns": "^3.0.0",
    "expo": "~54.0.0",
    "expo-file-system": "~19.0.17",
    "expo-image-picker": "~17.0.8",
    "expo-notifications": "~0.32.12",
    "expo-sharing": "~14.0.7",
    "expo-sqlite": "~16.0.9",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@types/react": "~19.1.10",
    "typescript": "^5.3.3"
  }
}
